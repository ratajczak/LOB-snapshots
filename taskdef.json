
{
    "containerDefinitions": [
      {
        "essential": true,
        "image": "$APP_IMAGE",
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group" : "$SERVICE_NAME-ecs-log-group",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "$SERVICE_NAME"
          }
        },
        "name": "$SERVICE_NAME",
        "environment": [
          {
            "name": "env",
            "value": "TEST"
          }
        ]
      }
    ],
    "cpu": "8",
    "executionRoleArn":
    "arn:aws:iam::$ACCOUNT_ID:role/$ROLE_NAME",
    "taskRoleArn": "arn:aws:iam::$ACCOUNT_ID:role/$ROLE_NAME",
    "family": "$ROLE_NAME-task",
    "memory": "2048",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ]
  }